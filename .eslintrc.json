{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["warn", "always"],
    "quotes": ["off", "double"],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-use-before-define": "off"
    

    }
}
